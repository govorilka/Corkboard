            <ITEXT FONTSIZE="20" CH="Рос­сия – чер­ни­ца"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="На пер­вой сед­ми­це по­ста."/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Вз­мы­ва­ет как пти­ца"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Свя­тая её кра­со­та."/>
            <para PARENT="Основной текст" ALIGN="0"/>
            <ITEXT FONTSIZE="20" CH="Как в по­ру мо­ле­ний­"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Ме­ня­ет­ся рус­ский на­род!"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="При­пав на ко­ле­ни -"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Он в рост бо­га­тыр­ский"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="встаёт."/>
            <para PARENT="Основной текст" ALIGN="0"/>
            <ITEXT FONTSIZE="20" CH="Ка­но­н по­ка­ян­ный­"/>
            <breakline/>
            <ITEXT FONTSIZE="19" CH="Хо­тя бы на ве­чер, на миг,"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="В ду­ше ока­ян­ной"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Ме­ня­ет личи­ну на лик."/>
            <para PARENT="Основной текст" ALIGN="0"/>
            <ITEXT FONTSIZE="20" CH="В­се­д­невно­го бы­та"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Сле­зает с неё ше­лу­ха."/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Сле­за­ми омы­та,"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Она пер­во­здан­но ти­ха."/>
            <para PARENT="Основной текст" ALIGN="0"/>
            <ITEXT FONTSIZE="20" CH="Как буд­то бы оп­том"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Собра­ли му­чи­тель­ный грех – "/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="И по­пран он, втоп­тан"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="В чер­не­ю­щий мартовский"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="снег."/>
            <para PARENT="Основной текст" ALIGN="0"/>
            <ITEXT FONTSIZE="20" CH="В­се – бра­тья и сёстры!"/>
            <breakline/>
            <ITEXT FONTSIZE="19" CH="В­се в звёз­дах церков­ных све­чей­!"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="В по­сту да­же воз­дух"/>
            <breakline/>
            <ITEXT FONTSIZE="20" CH="Жи­ви­тель­ней вся­ких ре­чей"/>
